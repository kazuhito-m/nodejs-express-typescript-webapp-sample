  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/commander": "^2.12.2",
    "@types/express": "^4.16.1",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/pg": "^7.4.11",
    "jest": "^24.0.0",
