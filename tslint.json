{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": [
            true,
            120
        ],
        "quotemark": [
            true,
            "single"
        ],
        "interface-name": [
            false
        ],
        "no-console": false,
        "no-require-imports": false,
        "no-var-requires": false,
        "arrow-parens": true
    },
    "rulesDirectory": []
}